// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model FlashCard {
  id String @default(uuid()) @id
  name String?   
  linkedlnUrl String?
  gitHubUrl  String? 
  createAt DateTime @default(now())
  updateAt DateTime?
}
